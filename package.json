{
  "name": "qiankun-vue",
  "version": "1.0.0",
  "description": "qiankun,demo,vue",
  "scripts": {
    "projects:install": "npm-run-all --serial install:*",
    "projects:start": "npm-run-all --parallel start:*",
    "projects:build": "npm-run-all --serial build:*",
    "install:vue-main": "cd projects/vue-main && npm i",
    "start:vue-main": "cd projects/vue-main && npm run start",
    "build:vue-main": "cd projects/vue-main && npm run build",
    "start:vue-hash": "cd projects/vue-hash && npm run start",
    "build:vue-hash": "cd projects/vue-hash && npm run build",
    "start:vue-history-cdn": "cd projects/vue-history-cdn && npm run start",
    "build:vue-history-cdn": "cd projects/vue-history-cdn && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wwhyes/qiankun-demo.git"
  },
  "keywords": [
    "qiankun",
    "vue2",
    "qiankun-vue",
    "qiankun-vue-demo"
  ],
  "author": "wwhyes",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wwhyes/qiankun-vue/issues"
  },
  "homepage": "https://github.com/wwhyes/qiankun-vue#readme",
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^2.6.10",
    "vue-router": "^3.1.2",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.0.4",
    "@vue/cli-plugin-eslint": "~4.0.4",
    "@vue/cli-plugin-router": "~4.0.4",
    "@vue/cli-plugin-vuex": "~4.0.4",
    "@vue/cli-service": "~4.0.4",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-vue": "^5.2.3",
    "npm-run-all": "^4.1.5",
    "vue-template-compiler": "^2.6.10"
  }
}
